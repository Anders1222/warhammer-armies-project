    <profile id="03e4-888d-e014-b28c" name="Marksman" publicationId="8ada-88a0-cdcd-9b5e" page="83" hidden="false" typeId="1a86-ad23-323a-d30c" typeName="Trait">
      <characteristics>
        <characteristic name="Special Rules" typeId="f6cc-ecc5-a476-801c">The character ignores penalties for shooting at long range and can re-roll failed rolls to Hit with missile weapons.</characteristic>
