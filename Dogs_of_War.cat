      </characteristics>
    </profile>
    <profile id="1915-ea75-30d5-3570" name="Drill Master" publicationId="8ada-88a0-cdcd-9b5e" page="83" hidden="false" typeId="1a86-ad23-323a-d30c" typeName="Trait">
      <characteristics>
