              <characteristics>
                <characteristic name="Special Rules" typeId="4627-cfbc-c37e-235a">Models in base contact with one or more models with this Gift suffer a -1 penalty to both WS and I. This has no effect on Daemons of Slaanesh.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="25.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="e1c7-1c7e-cbb4-d5f0" name="Spirit Swallower" page="" hidden="false" collective="false" import="true" type="upgrade">
