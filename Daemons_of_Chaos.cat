            <profile id="75b5-6c20-a856-0af0" name="Spirit Swallower" publicationId="ee54-3543-b9db-fe40" page="204" hidden="false" typeId="efb3-d0de-5567-f01a" typeName="Daemonic Gift">
              <characteristics>
                <characteristic name="Special Rules" typeId="4627-cfbc-c37e-235a">Roll a D6 for each unsaved wound the Daemon causes in close combat. On a 4+, the Daemon regains one wound lost earlier in the battle.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="50.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="6b71-8e1e-ad4f-010d" name="Temptator" page="" hidden="false" collective="false" import="true" type="upgrade">
