            <profile id="a43b-6db0-4eff-86e3" name="Unnatural Swiftness" publicationId="ee54-3543-b9db-fe40" page="204" hidden="false" typeId="efb3-d0de-5567-f01a" typeName="Daemonic Gift">
              <characteristics>
                <characteristic name="Special Rules" typeId="4627-cfbc-c37e-235a">The Daemon has Always Strikes First.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="25.0"/>
          </costs>
        </selectionEntry>
