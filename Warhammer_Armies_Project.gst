    <profile id="2a6c-20a9-1718-e49f" name="Anchan-Rogar the Soul Stealer" publicationId="9e23-79cb-pubN76150" page="170" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
