    <profile id="b7be-b86a-8409-1a7b" name="Song of Seduction" publicationId="eee5-f9c5-fa08-3f2e" page="114, 7, 240" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
