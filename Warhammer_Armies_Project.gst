      </characteristics>
    </profile>
    <profile id="4c51-7bd8-93b6-cac6" name="Bolt of Change" publicationId="eee5-f9c5-fa08-3f2e" page="112, 5, 238" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
