    <profile id="202f-4dfc-6064-f53b" name="Blades of Putrification" publicationId="eee5-f9c5-fa08-3f2e" page="113, 200, 239" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
