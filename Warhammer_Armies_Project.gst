    </profile>
    <profile id="83ce-8b5d-eef3-f48d" name="Doombolt of Kharaidon" publicationId="9e23-79cb-pubN76150" page="168" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
