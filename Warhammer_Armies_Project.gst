    <profile id="1cf4-60b2-c05f-5ce5" name="Transmutation of Lead (Bound Spell 12+)" publicationId="9e23-79cb-pubN66727" page="146" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
