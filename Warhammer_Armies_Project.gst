      </characteristics>
    </profile>
    <profile id="82f8-dd54-9293-63b2" name="Walk Between Worlds" publicationId="b724-d2c4-d151-2d9e" page="201, 187" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
