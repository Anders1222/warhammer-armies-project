    </profile>
    <profile id="1d55-f70f-d713-0f87" name="Fireball (Bound Spell)" publicationId="9e23-79cb-pubN66727" page="140" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
