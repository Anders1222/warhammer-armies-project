    <profile id="1146-2b63-8bfc-ab96" name="Stream of Corruption" publicationId="eee5-f9c5-fa08-3f2e" page="113, 200, 239" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
