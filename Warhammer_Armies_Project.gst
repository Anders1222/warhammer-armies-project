    <profile id="6029-2904-b7c5-17fe" name="Nagaelythe the Chillwind" publicationId="9e23-79cb-pubN76150" page="170" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
