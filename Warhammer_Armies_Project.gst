    <profile id="7ebe-8da7-4994-41d7" name="Vanhel&apos;s Danse Macabre" publicationId="4ff3-b09f-ec27-d6ec" page="237, 27" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
