      </characteristics>
    </profile>
    <profile id="2886-1c9b-c241-1093" name="The Savage Beast of Horros" publicationId="9e23-79cb-pubN66727" page="148" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
