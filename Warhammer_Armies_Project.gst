    <profile id="2897-e2c7-a845-ee02" name="Doombolt of Kharaidon (Bound Spell 4+)" publicationId="9e23-79cb-pubN76150" page="168" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
