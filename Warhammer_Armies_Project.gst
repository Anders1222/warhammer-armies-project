    <profile id="9579-8aa8-6eff-8a6a" name="The Speed of Light (Bound Spell 5+)" publicationId="9e23-79cb-pubN66727" page="150" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
