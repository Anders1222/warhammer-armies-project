    <profile id="6e49-e2c5-e1f2-6d5c" name="Invocation of Nehek" publicationId="4ff3-b09f-ec27-d6ec" page="237, 27" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
